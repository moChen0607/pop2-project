		menuItem -optionBox true -i "reverseSurface.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kReverseSurfacesOptionsAnnot")) 
			-c "ReverseSurfaceDirectionOptions"
			reverseDialogItem;
    
	setParent -m ..;
}