// Creation Date:  17 June 2014
//
//
//  Procedure Name:
//      ModelingSurfacesMenu
//
//  Description:
//		Create the MODELING->Surfaces menu
//
//  Input Arguments:
//      Parent window that menu will be a part of.
//
//  Return Value:
//      None.
//

global proc ModelingSurfacesMenu ( string $parent ) 
//
// Description:
//		Modeling Surfaces menu
//
{
	if (! `isTrue "SurfaceUIExists"`) return;

	setParent -m $parent;
	if( `menu -q -ni $parent` != 0 ) {
		//
		//	Menu is built already - just return
		//
		return;
	}

	int $isAdvModel = `licenseCheck -mode "edit" -type "model"`;

	menuItem -divider true -dividerLabel (uiRes("m_ModelingSurfacesMenu.kCreateDivider"));
		
		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kLoft")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kLoftAnnot"))
			-c "Loft"
			-dmc "loftToolScript 2"
			-i "skin.png"
			surfLoftItem;
		
		menuItem -optionBox true 
			-label (uiRes("m_ModelingSurfacesMenu.kLoftOptions"))
			-annotation (uiRes("m_ModelingSurfacesMenu.kLoftOptionsAnnot"))
			-c "LoftOptions"
			-i "skin.png"
			loftDialogItem;
		
		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kPlanar")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kPlanarAnnot"))
			-c "Planar"
			-dmc "planarToolScript 2"
			-i "planarTrim.png"
			planarTrimItem;

		menuItem -optionBox true -i "planarTrim.png"
			-label (uiRes("m_ModelingSurfacesMenu.kPlanarOptions")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kPlanarOptionsAnnot")) 
			-c "PlanarOptions"
			planerTrimDialogItem;
			
		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kRevolve")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kRevolveAnnot"))
			-c "Revolve"
			-dmc "revolveToolScript 2"
			-i "revolve.png"
			surfRevolveItem;

		menuItem -optionBox true
			-label (uiRes("m_ModelingSurfacesMenu.kRevolveOptions"))
			-annotation (uiRes("m_ModelingSurfacesMenu.kRevolveOptionsAnnot"))
			-c "RevolveOptions"
			-i "revolve.png"
			revolveDialogItem;
		
		menuItem -d true -ld false;

		menuItem -label (uiRes("m_ModelingSurfacesMenu.kBirail"))  -sm 1 -to 1 -aob true;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kBirail1Tool")) 
				-c "Birail1"
				-dmc "birailToolScript 2 1"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail1ToolAnnot"))
				-image "birail1Gen.png"
				spBirailItem;

			menuItem -optionBox true -image "birail1Gen.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail1ToolOptionsAnnot")) 
				-c "Birail1Options"
				birail1ToolDialog;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kBirail2Tool"))
				-c "Birail2"
				-dmc "birailToolScript 2 2"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail2ToolAnnot"))
				-image "birail2Gen.png"
				dpBirailItem;

			menuItem -optionBox true -image "birail2Gen.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail2ToolOptionsAnnot")) 
				-c "Birail2Options"
				birail2ToolDialog;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kBirail3Tool"))
				-c "Birail3"
				-dmc "birailToolScript 2 3"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail3ToolAnnot"))
				-image "birail3Gen.png"
				mpBirailItem;

			menuItem -optionBox true -image "birail3Gen.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kBirail3ToolOptionsAnnot")) 
				-c "Birail3Options"
				birail3ToolDialog;

		setParent -m .. ;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kExtrude")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kExtrudeAnnot"))
			-c "Extrude"
			-dmc "extrudeToolScript 2"
			-i "extrude.png"
			surfExtrudeItem;

		menuItem -optionBox true 
			-label (uiRes("m_ModelingSurfacesMenu.kExtrudeOptions"))
			-annotation (uiRes("m_ModelingSurfacesMenu.kExtrudeOptionsAnnot"))
			-c "ExtrudeOptions"
			-i "extrude.png"
			extrudeDialogItem;

		menuItem -d true -ld false;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kBoundary")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kBoundaryAnnot"))
			-c "Boundary"
			-dmc "boundaryToolScript 2"
			-i "boundary.png"
			surfBoundaryItem;

		menuItem -optionBox true
			-label (uiRes("m_ModelingSurfacesMenu.kBoundaryOptions"))
			-annotation (uiRes("m_ModelingSurfacesMenu.kBoundaryOptionsAnnot"))
			-c "BoundaryOptions"
			-i "boundary.png"
			boundaryDialogItem;
			
		if( `licenseCheck -m edit -type model` ) {
			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kSquare")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kSquareAnnot"))
				-c "SquareSurface"
				-dmc "squareSrfToolScript 2"
				-i "squareSrf.png"
				surfSquareItem;

			menuItem -optionBox true
				-label (uiRes("m_ModelingSurfacesMenu.kSquareOptions"))
				-annotation (uiRes("m_ModelingSurfacesMenu.kSquareOptionsAnnot"))
				-c "SquareSurfaceOptions"
				-i "squareSrf.png"
				squareDialogItem;
		}

		menuItem -d true -ld false;

		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kBevel"))  
			-annotation (uiRes("m_ModelingSurfacesMenu.kBevelAnnot")) 
			-c "Bevel"
			-dmc "bevelToolScript 2"
			-i "bevel.png"
			surfBevelItem;

		menuItem -optionBox true 
			-label (uiRes("m_ModelingSurfacesMenu.kBevelOptions")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kBevelOptionsAnnot"))
			-c "BevelOptions"
			-i "bevel.png"
			bevelDialogItem;
		
		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kBevelPlus"))  
			-annotation (uiRes("m_ModelingSurfacesMenu.kBevelPlusAnnot"))
			-c "BevelPlus"
			-dmc "bevelPlusToolScript 2"
			-i "bevelPlus.png"
			surfBevelPlusItem;

		menuItem -optionBox true 
			-label (uiRes("m_ModelingSurfacesMenu.kBevelPlusOptions")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kBevelPlusOptionsAnnot"))
			-c "BevelPlusOptions"
			-i "bevelPlus.png"
			bevelPlusDialogItem;

	menuItem -divider true -dividerLabel (uiRes("m_ModelingSurfacesMenu.kEditNurbsSurfacesDivider"));
		
		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kDuplicateNurbsPatches")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kDuplicateNurbsPatchesAnnot"))
			-c "DuplicateNURBSPatches"
			-dmc "duplicateSurfaceToolScript 2"
			-i "duplicateSurface.png"
			duplSrfItem;

		menuItem -optionBox true -i "duplicateSurface.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kDuplicateNurbsPatchesOptionsAnnot"))
			-c "DuplicateNURBSPatchesOptions"
			duplSrfDialog;

		menuItem -d true -ld false;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kAlignSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kAlignSurfacesAnnot"))
			-c "AlignSurfaces"
			-dmc "alignSurfaceToolScript 2"
			-i "alignSurface.png" 
			alignItem;

		menuItem -optionBox true -i "alignSurface.png" 
			-annotation (uiRes("m_ModelingSurfacesMenu.kAlignSurfacesOptionsAnnot"))
			-c "AlignSurfacesOptions"
			alignDialogItem;
		
		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kAttachSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kAttachSurfacesAnnot"))
			-c "AttachSurfaces"
			-dmc "attachSrfToolScript 2"
			-i "align.png"
			attachSrfItem;

		menuItem -optionBox true -i "align.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kAttachSurfacesOptionsAnnot"))
			-c "AttachSurfacesOptions"
			attachSrfDialog;

		menuItem -label (uiRes("m_ModelingSurfacesMenu.kAttachWithoutMoving"))  
			-annotation (uiRes("m_ModelingSurfacesMenu.kAttachWithoutMovingAnnot")) 
			-i "attachWithoutMoving.png"
			-c "attachWithoutMoving"
			attachWithoutMovingItem;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kDetachSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kDetachSurfacesAnnot"))
			-c "DetachSurfaces"
			-dmc "detachSurfaceToolScript 2"
			-i "detach.png"
			detachItem;

		menuItem -optionBox true -i "detach.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kDetachSurfacesOptionsAnnot"))
			-c "DetachSurfacesOptions"
			detachDialog;

		menuItem -label (uiRes("m_ModelingSurfacesMenu.kMoveSeam"))  
			-annotation (uiRes("m_ModelingSurfacesMenu.kMoveSeamAnnot")) 
			-i "moveSeam.png"
			-c "moveNurbsSurfaceSeam" 
			moveSurfaceSeamItem;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kOpenCloseSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kOpenCloseSurfacesAnnot"))
			-c "OpenCloseSurfaces"
			-dmc "closeSurfaceToolScript 2"
			-i "openCloseSurface.png"
			closeItem;

		menuItem -optionBox true -i "openCloseSurface.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kOpenCloseSurfacesOptionsAnnot"))
			-c "OpenCloseSurfacesOptions"
			closeDialogItem;
		
		menuItem -d true -ld false; 

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kIntersectSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kIntersectSurfacesAnnot"))
			-c "IntersectSurfaces"
			-dmc "intersectToolScript 2"
			-i "srfIntersect.png"
			intersectItem;

		menuItem -optionBox true 
			-annotation (uiRes("m_ModelingSurfacesMenu.kIntersectSurfacesOptionsAnnot"))
			-i "srfIntersect.png"
			-c "IntersectSurfacesOptions"
			intersectDialogItem;
		
		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kProjectCurveOnSurface")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kProjectCurveOnSurfaceAnnot"))
			-c "ProjectCurveOnSurface"
			-dmc "projectToolScript 2"
			-i "projectCurve.png"
			projectCurveItem;

		menuItem -optionBox true
			-annotation (uiRes("m_ModelingSurfacesMenu.kProjectCurveOnSurfaceOptionsAnnot"))
			-i "projectCurve.png"
			-c "ProjectCurveOnSurfaceOptions"
			projectCurveDialogItem;

		menuItem -label (uiRes("m_ModelingSurfacesMenu.kTrimTool"))  
			-annotation (uiRes("m_ModelingSurfacesMenu.kTrimToolAnnot")) 
			-image "trim.png"
			-c "TrimTool" 
			trimSurfaceItem;

		menuItem -optionBox true -image "trim.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kTrimToolOptionsAnnot")) 
			-c "TrimToolOptions" 
			trimDialogItem;

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kUntrimSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kUntrimSurfacesAnnot"))
			-c "UntrimSurfaces"
			-dmc "untrimToolScript 2"
			-image "untrim.png"
			untrimItem;

		menuItem -optionBox true -image "untrim.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kUntrimSurfacesOptionsAnnot")) 
			-c "UntrimSurfacesOptions"
			untrimDialogItem;
		
		menuItem -d true -ld false;

		if( $isAdvModel ) {
			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kExtendSurfaces")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kExtendSurfacesAnnot")) 
				-c "ExtendSurfaces"
				-dmc "extendSurfaceToolScript 2"
				-i "extendSurface.png"
				extendSrfItem;

			menuItem -optionBox true -i "extendSurface.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kExtendSurfacesOptionsAnnot")) 
				-c "ExtendSurfacesOptions"
				extendSurfaceDialogItem;
		}

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kInsertIsoparms")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kInsertIsoparmsAnnot"))
			-c "InsertIsoparms"
			-dmc "insertSurfaceToolScript 2"
			-i "insert.png"
			insertItem;

		menuItem -optionBox true -i "insert.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kInsertIsoparmsOptionsAnnot"))
			-c "InsertIsoparmsOptions"
			insertDialog;

		if( $isAdvModel ) {
			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kOffsetSurfaces")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kOffsetSurfacesAnnot")) 
				-c "OffsetSurfaces"
				-dmc "offsetSurfaceToolScript 2"
				-i "offsetSurface.png"
				surfOffsetItem;

			menuItem -optionBox true
				-annotation (uiRes("m_ModelingSurfacesMenu.kOffsetSurfacesOptionsAnnot")) 
				-i "offsetSurface.png"
				-c "OffsetSurfacesOptions"
				offsetSurfaceDialogItem;
		}

		menuItem -d true -ld false;

		if( $isAdvModel ) {
			menuItem -label (uiRes("m_ModelingSurfacesMenu.kRoundTool")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kRoundToolAnnot")) 
				-image "roundSrf.png"
				-c "RoundTool" 
				roundItem;

			menuItem -optionBox true -image "roundSrf.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kRountToolOptionsAnnot")) 
				-c "RoundToolOptions"
				roundOptionItem;
		}

		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kStitch"))  
			-sm 1 -to 1 -aob true;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kStitchSurfacePoints")) 
				-c "StitchSurfacePoints"
				-dmc "performStitchSurfacePoints(2)"
				-annotation (uiRes("m_ModelingSurfacesMenu.kStitchSurfacePointsAnnot"))
				-i "stitchSurfacePoints.png"
				stitchSurfacePointsItem;

			menuItem -optionBox true
				-annotation (uiRes("m_ModelingSurfacesMenu.kStitchSurfacePointsOptionsAnnot")) 
				-i "stitchSurfacePoints.png"
				-c "StitchSurfacePointsOptions"
				stitchSurfacePointsDialogItem;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kStitchEdgesTool")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kStitchEdgesToolAnnot")) 
				-image "stitchSrf.png"
				-c "StitchEdgesTool" 
				surfStitchItem;

			menuItem -optionBox true -image "stitchSrf.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kStitchToolOptionsAnnot")) 
				-c "StitchEdgesToolOptions"
				stitchSrfDialogItem;

			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kGlobalStitch")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kGlobalStitchAnnot")) 
				-c "GlobalStitch"
				-dmc "globalStitchToolScript 2"
				-image "globalStitch.png"
				globalStitchItem;

			menuItem -optionBox true
				-annotation (uiRes("m_ModelingSurfacesMenu.kGlobalStitchOptionsAnnot")) 
				-image "globalStitch.png"
				-c "GlobalStitchOptions"
				globalStitchDialogItem;

		setParent -m .. ;

		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kSurfaceFillet")) 
			-sm 1 -to 1 -aob true;

			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kCircularFillet")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kCircularFilletAnnot")) 
				-c "CircularFillet"
				-dmc "circFilletToolScript 2"
				-i "surfaceFillet.png"
				circularFilletItem;

			menuItem -optionBox true
				-annotation (uiRes("m_ModelingSurfacesMenu.kCircularFilletOptionsAnnot"))
				-i "surfaceFillet.png"
				-c "CircularFilletOptions"
				circularFilletDialogItem;

			menuItem
				-label (uiRes("m_ModelingSurfacesMenu.kFreeformFillet")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kFreeformFilletAnnot"))
				-c "FreeformFillet"
				-dmc "freeFilletToolScript 2"
				-i "filletFreeform_S.png"
				freeFormFilletItem ;

			menuItem -optionBox true
				-annotation (uiRes("m_ModelingSurfacesMenu.kFreeformFilletOptionsAnnot")) 
				-i "filletFreeform_S.png"
				-c "FreeformFilletOptions"
				freeFormFilletDialogItem;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kFilletBlendTool")) 
       			-c "FilletBlendTool"
       			-dmc "blendToolScript 2"
       			-annotation (uiRes("m_ModelingSurfacesMenu.kFilletBlendToolAnnot"))
       			-image "blendSurface.png"
				blendItem;

			menuItem -optionBox true -image "blendSurface.png"
        		-annotation (uiRes("m_ModelingSurfacesMenu.kFilletBlendToolOptionsAnnot")) 
				-c "FilletBlendToolOptions"
				blendDialogItem;

		setParent -m .. ;
		
		menuItem -d true -ld false;

		menuItem
		    -label (uiRes("m_ModelingSurfacesMenu.kSculptGeometryTool")) 
			-annotation (getRunTimeCommandAnnotation("SculptGeometryTool"))
		    -image "putty.png"
		    -command "SculptGeometryTool"
		    -dragDoubleClickCommand "SculptGeometryToolOptions"
		    ;
	  
		menuItem -optionBox true
			-annotation (getRunTimeCommandAnnotation("SculptGeometryToolOptions"))
			-image "putty.png"
			-command "SculptGeometryToolOptions"
			;

		menuItem 
			-label (uiRes("m_ModelingSurfacesMenu.kSurfaceEditing")) 
			-sm 1 -to 1 -aob true;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kSurfaceEditingTool")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kSurfaceEditingToolAnnot")) 
				-image "surfaceEditor.png"
				-c "SurfaceEditingTool"
				surfaceEditorItem;

			menuItem -optionBox true -image "surfaceEditor.png"
				-annotation (uiRes("m_ModelingSurfacesMenu.kSurfaceEditingToolOptionsAnnot")) 
				-c "SurfaceEditingToolOptions"
				surfaceEditorDialogItem;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kBreakTangent")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kBreakTangentAnnot"))
				-i "breakSrfTangent.png"
				-c "BreakTangent"
				breakTangentItem;

			menuItem -label (uiRes("m_ModelingSurfacesMenu.kSmoothTangent")) 
				-annotation (uiRes("m_ModelingSurfacesMenu.kSmoothTangentAnnot"))
				-i "smoothTangent.png"
				-c "SmoothTangent"
				smoothTangentItem;

		setParent -m ..;

		menuItem -d true -ld false;

		if( $isAdvModel ) {
			menuItem 
				-label (uiRes("m_ModelingSurfacesMenu.kBooleans"))  
				-sm 1 -to 1 -aob true;	

				menuItem -label (uiRes("m_ModelingSurfacesMenu.kUnionTool")) 
					-c "SurfaceBooleanUnionTool"
					-dmc "nurbsBooleanToolScript 2 0"
					-annotation (uiRes("m_ModelingSurfacesMenu.kUnionToolAnnot")) 
					-image "nurbsShellUnion.png"
					booleanUnionItem;

				menuItem -optionBox true -image "nurbsShellUnion.png"
					-annotation (uiRes("m_ModelingSurfacesMenu.kUnionOptionsAnnot")) 
					-c "SurfaceBooleanUnionToolOptions"
					unionOptItem;

				menuItem -label (uiRes("m_ModelingSurfacesMenu.kDifferenceTool")) 
					-c "SurfaceBooleanSubtractTool"
					-dmc "nurbsBooleanToolScript 2 1"
					-annotation (uiRes("m_ModelingSurfacesMenu.kDifferenceToolAnnot")) 
					-image "nurbsShellSubtract.png"
					booleanSubtractItem;

				menuItem -optionBox true -image "nurbsShellSubtract.png"
					-annotation (uiRes("m_ModelingSurfacesMenu.kDifferenceOptionsAnnot")) 
					-c "SurfaceBooleanSubtractToolOptions"
					subtractOptItem;

				menuItem -label (uiRes("m_ModelingSurfacesMenu.kIntersectionTool")) 
					-c "SurfaceBooleanIntersectTool"
					-dmc "nurbsBooleanToolScript 2 2"
					-annotation (uiRes("m_ModelingSurfacesMenu.kIntersectionToolAnnot")) 
					-image "nurbsShellIntersect.png"
					booleanIntersectItem;

				menuItem -optionBox true -image "nurbsShellIntersect.png"
					-annotation (uiRes("m_ModelingSurfacesMenu.kIntersectionOptionsAnnot")) 
					-c "SurfaceBooleanIntersectToolOptions"
					intersectOptItem;

			setParent -m ..;

			menuItem -d true -ld false;
		}

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kRebuildSurfaces")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kRebuildSurfacesAnnot")) 
			-c "RebuildSurfaces"
			-dmc "rebuildSurfaceToolScript 2"
			-i "rebuildSurface.png"
			rebuildSrfItem;

		menuItem -optionBox true -i "rebuildSurface.png"
			-annotation (uiRes("m_ModelingSurfacesMenu.kRebuildSurfacesOptionsAnnot")) 
			-c "RebuildSurfacesOptions"
			rebuildSurfaceDialogItem; 

		menuItem
			-label (uiRes("m_ModelingSurfacesMenu.kReverseSurfaceDirection")) 
			-annotation (uiRes("m_ModelingSurfacesMenu.kReverseSurfaceDirectionAnnot"))
			-c "ReverseSurfaceDirection"
			-dmc "reverseSurfaceToolScript 2"
			-i "reverseSurface.png"
