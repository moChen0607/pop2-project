<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEprojection'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='image' type='maya.float3'>
			<label>Image</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEprojection'>
		<property name='outColor'/>
		<property name='image'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEprojection'>
		<property name='outColor'/>
	</view>
</templates>