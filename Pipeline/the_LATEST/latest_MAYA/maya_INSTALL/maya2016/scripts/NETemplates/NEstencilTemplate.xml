<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEstencil'>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='image' type='maya.float3'>
			<label>Image</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEstencil'>
		<property name='outAlpha'/>
		<property name='image'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEstencil'>
		<property name='outAlpha'/>
	</view>
</templates>