<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEcontrast'>
		<attribute name='outValue' type='maya.float3'>
			<label>Out Value</label>
		</attribute>
		<attribute name='contrast' type='maya.float3'>
			<label>Contrast</label>
		</attribute>
		<attribute name='value' type='maya.float3'>
			<label>Value</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcontrast'>
		<property name='outValue'/>
		<property name='contrast'/>
		<property name='value'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEcontrast'>
		<property name='outValue'/>
	</view>
</templates>