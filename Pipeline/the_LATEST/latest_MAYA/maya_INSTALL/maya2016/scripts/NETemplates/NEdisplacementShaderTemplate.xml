<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEdisplacementShader'>
		<attribute name='displacement' type='maya.float'>
			<label>Displacement</label>
		</attribute>
		<attribute name='vectorDisplacement' type='maya.float3'>
			<label>Vector Displacement</label>
		</attribute>
		<attribute name='tangent' type='maya.float3'>
			<label>Tangent</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEdisplacementShader'>
		<property name='displacement'/>
		<property name='vectorDisplacement'/>
		<property name='tangent'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEdisplacementShader'>
		<property name='displacement'/>
	</view>
</templates>