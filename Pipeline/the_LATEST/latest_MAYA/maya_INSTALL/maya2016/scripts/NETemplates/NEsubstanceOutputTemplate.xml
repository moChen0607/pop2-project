<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEsubstanceOutput'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='inColor' type='maya.float3'>
			<label>In Color</label>
		</attribute>
		<attribute name='inAlpha' type='maya.float'>
			<label>In Alpha</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEsubstanceOutput'>
		<property name='outColor'/>
		<property name='outAlpha'/>
		<property name='inColor'/>
		<property name='inAlpha'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEsubstanceOutput'>
		<property name='outColor'/>
	</view>
</templates>