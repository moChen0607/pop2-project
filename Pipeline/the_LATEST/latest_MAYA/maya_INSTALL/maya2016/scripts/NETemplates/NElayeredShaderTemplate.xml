<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NElayeredShader'>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='inputs' type='maya.TdataCompound'>
			<label>Inputs</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NElayeredShader'>
		<property name='outColor'/>
		<property name='inputs'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NElayeredShader'>
		<property name='outColor'/>
	</view>
</templates>