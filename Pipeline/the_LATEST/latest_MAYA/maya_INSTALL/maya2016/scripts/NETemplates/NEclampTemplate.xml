<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEclamp'>
		<attribute name='output' type='maya.float3'>
			<label>Output</label>
		</attribute>
		<attribute name='input' type='maya.float3'>
			<label>Input</label>
		</attribute>
		<attribute name='min' type='maya.float3'>
			<label>Min</label>
		</attribute>
		<attribute name='max' type='maya.float3'>
			<label>Max</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEclamp'>
		<property name='output'/>
		<property name='input'/>
		<property name='min'/>
		<property name='max'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEclamp'>
		<property name='output'/>
	</view>
</templates>