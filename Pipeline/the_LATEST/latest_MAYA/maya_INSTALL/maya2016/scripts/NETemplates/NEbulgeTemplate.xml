<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEbulge'>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='uvCoord' type='maya.float2'>
			<label>Uv Coord</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEbulge'>
		<property name='outAlpha'/>
		<property name='outColor'/>
		<property name='uvCoord'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEbulge'>
		<property name='outColor'/>
	</view>
</templates>