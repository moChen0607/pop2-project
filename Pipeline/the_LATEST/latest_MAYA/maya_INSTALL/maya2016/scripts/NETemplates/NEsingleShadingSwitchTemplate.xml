<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEsingleShadingSwitch'>
		<attribute name='output' type='maya.float'>
			<label>Output</label>
		</attribute>
		<attribute name='default' type='maya.float'>
			<label>Default</label>
		</attribute>
		<attribute name='input' type='maya.TdataCompound'>
			<label>Input</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEsingleShadingSwitch'>
		<property name='output'/>
		<property name='default'/>
		<property name='input'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEsingleShadingSwitch'>
		<property name='output'/>
	</view>
</templates>