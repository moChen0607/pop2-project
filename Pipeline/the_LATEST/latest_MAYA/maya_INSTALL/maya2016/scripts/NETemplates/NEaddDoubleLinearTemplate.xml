<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEaddDoubleLinear'>
		<attribute name='output' type='maya.doubleLinear'>
			<label>Output</label>
		</attribute>
		<attribute name='input1' type='maya.doubleLinear'>
			<label>Input 1</label>
		</attribute>
		<attribute name='input2' type='maya.doubleLinear'>
			<label>Input 2</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEaddDoubleLinear'>
		<property name='output'/>
		<property name='input1'/>
		<property name='input2'/>
	</view>
	<view name='NEDefaultSoloOutput' template='NEaddDoubleLinear'>
		<property name='output'/>
	</view>
</templates>