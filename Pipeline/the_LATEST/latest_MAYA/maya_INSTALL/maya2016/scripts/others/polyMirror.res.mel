displayString -replace -value "polyMirror: Error specifying X direction.  Valid values are -1, 0, 1" m_polyMirror.kErrorXDir;
displayString -replace -value "polyMirror: Error specifying Y direction.  Valid values are -1, 0, 1" m_polyMirror.kErrorYDir;
displayString -replace -value "polyMirror: Error specifying Z direction.  Valid values are -1, 0, 1" m_polyMirror.kErrorZDir;
displayString -replace -value "Mirroring along 2 directions might yield unpredictable results" m_polyMirror.kWarnMirror2Dir;
displayString -replace -value "Multiple objects cannot be mirrored.  Using the first one" m_polyMirror.kWarnMultipleObjs;
displayString -replace -value "polyMirror: Cannot stitch closed objects" m_polyMirror.kWarnStichClosed;
