global proc setFluidEmitterOptionVars (int $setFactorySettings)
{
	if( $setFactorySettings || !`optionVar -exists flEmitterName` ) 
	{
		optionVar -stringValue flEmitterName ""; 
	} 

	if( $setFactorySettings || !`optionVar -exists flEmitterParenting` ) 
	{
		optionVar -intValue flEmitterParenting 1; 
	} 

	if( $setFactorySettings || !`optionVar -exists flEmitterTypeOM` ) 
	{
		optionVar -stringValue flEmitterTypeOM (uiRes("m_dynCreateFluidLayout.kOmni")); 
	} 
	if( $setFactorySettings || !`optionVar -exists flEmitterDensityRate` ) 
	{
		optionVar -floatValue flEmitterDensityRate 1.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterHeatRate` ) 
	{
		optionVar -floatValue flEmitterHeatRate 1.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterFuelRate` ) 
	{
		optionVar -floatValue flEmitterFuelRate 1.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterFluidDropoff` ) 
	{
		optionVar -floatValue flEmitterFluidDropoff 2.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterCycleOM` ) 
	{
		optionVar -intValue flEmitterCycleOM 1; 
	} 
	if( $setFactorySettings || !`optionVar -exists flEmitterCycleInterval` ) 
	{
		optionVar -intValue flEmitterCycleInterval 1; 
	} 

	if( $setFactorySettings || !`optionVar -exists flEmitterMaxDistance` ) 
	{
		optionVar -floatValue flEmitterMaxDistance 1.0; 
	}
	if( $setFactorySettings || !`optionVar -exists flEmitterMinDistance` ) 
	{
		optionVar -floatValue flEmitterMinDistance 0.0; 
	}  

	if( $setFactorySettings || !`optionVar -exists flEmitterVolumeShapeOM` ) 
	{
		optionVar -intValue flEmitterVolumeShapeOM 1; 
	} 
	if( $setFactorySettings || !`optionVar -exists flEmitterVolumeOffsetX` ) 
	{
		optionVar -floatValue flEmitterVolumeOffsetX 0.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterVolumeOffsetY` ) 
	{
		optionVar -floatValue flEmitterVolumeOffsetY 0.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterVolumeOffsetZ` ) 
	{
		optionVar -floatValue flEmitterVolumeOffsetZ 0.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterVolumeSweep` ) 
	{
		optionVar -floatValue flEmitterVolumeSweep 360.0; 
	}  
	if( $setFactorySettings || !`optionVar -exists flEmitterTorusSectionRadius` ) 
	{
		optionVar -floatValue flEmitterTorusSectionRadius 0.5; 
	}  
}

