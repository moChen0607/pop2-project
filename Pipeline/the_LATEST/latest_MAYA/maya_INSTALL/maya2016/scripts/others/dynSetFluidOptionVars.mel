global proc setExtendFluidOptionVars( int $setFactorySettings )
{
	if( $setFactorySettings || !`optionVar -exists "extendFluidRebuildIC"` ) 
	{
		optionVar -intValue "extendFluidRebuildIC" 1; 
	}

	if( $setFactorySettings || !`optionVar -exists "extendFluidMinY"` ) 
	{
		optionVar -intValue "extendFluidMinY" 0; 
	}
	if( $setFactorySettings || !`optionVar -exists "extendFluidMaxY"` ) 
	{
		optionVar -intValue "extendFluidMaxY" 2; 
	}

	if( $setFactorySettings || !`optionVar -exists "extendFluidMinX"` ) 
	{
		optionVar -intValue "extendFluidMinX" 0; 
	}
	if( $setFactorySettings || !`optionVar -exists "extendFluidMaxX"` ) 
	{
		optionVar -intValue "extendFluidMaxX" 0; 
	}
	if( $setFactorySettings || !`optionVar -exists "extendFluidMinZ"` ) 
	{
		optionVar -intValue "extendFluidMinZ" 0; 
	}
	if( $setFactorySettings || !`optionVar -exists "extendFluidMaxZ"` ) 
	{
		optionVar -intValue "extendFluidMaxZ" 0; 
	}

}

