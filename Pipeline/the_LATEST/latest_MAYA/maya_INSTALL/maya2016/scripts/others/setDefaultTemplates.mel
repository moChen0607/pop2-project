//global proc setDefaultTemplates ()
{
//
//	This is the formating template. It does ALL the layout for this window
//	and ensures everything lines up graphically and look pretty. The point
//	of this file is very simple, change graphic layout in one location and
//	all windows will reflect the changes.
//

// Declared as a global variable so windows can be set to the correct size based
// upon the preference of two font sizes.

// Single Widget Width is declared global so that users of
// this template can compute a widget width which is platform
// independent that will fit in the template properly.
int $kBigFont = true;

//
//	--- BEGIN: Only the values below should be referenced outside this file ---
//

//	For the purposes of aligning controls with the DefaultTemplate you
//	should only be referencing the following global variables outside of this
//	file.
//
global int $gDefaultTemplateLabelWidth;
global int $gDefaultTemplateLabelOffset;
global int $gDefaultTemplateSingleWidgetWidth;
global int $gDefaultTemplateExtraWidth;

//	For the purposes of aligning controls with the attributeEditorTemplate you
//	should only be referencing the following global variables outside of this
//	file.
//
global int $gAttributeEditorTemplateLabelWidth;
global int $gAttributeEditorTemplateLabelOffset;
global int $gAttributeEditorTemplateSingleWidgetWidth;
global int $gAttributeEditorTemplateExtraWidth;

//	For similar values regarding the OptionsTemplate see below where the
//	template is actually defined.

//
//	--- END: Only the values above should be referenced outside this file ---
//


//	NOTE: Do not reference any value below outside of this file. Any global variable 
//	should be changed to be non-global.
//
global int $kSingleWidgetWidthIndex = 62;
global int $kTextColumnWidthIndex = 120;
int $kSecondColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
int $kThirdColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
int $kFourthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
int $kFifthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
int $kSliderWidgetWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
int $kUnitWidthIndex = 20;
int $kLongWordExtra = 0;
global int $gStandardIdent = 5;
$gStandardIdent = 5;
global int $gStandardFrameLayoutMarginWidth = 3; // This margin width should be applied to most frame layout, therefore it 
												 // will give us a nice hierarchy for nested frame layouts. 

int $kThreeButtonRadioExtra = 42;
int $kOffsetText = (`about -mac` ? 8 : 5);
int $kHeightIndex = 26; // this is used for some controls in the 
			// attribute editor
int $kMapArrowIndex = 20;
int $kMapButtonIndex = ($kSingleWidgetWidthIndex - $kMapArrowIndex);
int $kScrollBar = 14;
int $kMagicSpacerNumber = 30; // For some reason I need this to get the correct window width
			      // I will debug this later when I have more time

global int $gStandardWindowWidthIndex;
$gStandardWindowWidthIndex = ($gStandardIdent +
				$kTextColumnWidthIndex + 
				( 3 * $kSingleWidgetWidthIndex ) +
				$kUnitWidthIndex +
				$gStandardIdent +
				$kMagicSpacerNumber +
				$kScrollBar);
global int $gStandardWidgetAreaIndex;
$gStandardWidgetAreaIndex = ($gStandardWindowWidthIndex -
				($gStandardIdent +  
				$kTextColumnWidthIndex +
				$gStandardIdent + $kScrollBar));

// the following global ints are defined, so the channelbox, which is 
// used in the attribute editor, can adjust to the correct layout.
// this is necessary, because the channel box has no ability to be
// layed out through userTemplates. The values have to be hardcoded.
// This is a workaround for that problem.

// Global used by option box scripts
//
global int $gOptionBoxTextColumnWidthIndex;
$gOptionBoxTextColumnWidthIndex = $kTextColumnWidthIndex;

global int $gChannelBoxWidgetWidthIndex;
$gChannelBoxWidgetWidthIndex = $kTextColumnWidthIndex + 
		(3 * $kSingleWidgetWidthIndex);

global int $gChannelBoxWidgetHeightIndex;
$gChannelBoxWidgetHeightIndex = 200;

// Define size of the name field in the Attribute Editor
global int $gAENameTextFieldIndex;
$gAENameTextFieldIndex = 161;

global int $gTextColumnWidthIndex;
global int $gAESingleWidgetWidthIndex;
global int $gAEMapButtonIndex;

//	This global variable specifies the offset that should be used for 
//	offsetting colour swatches. This value will make a colour swatch
//	align nicely with fields.
//
global int $gTemplateColorSwatchOffset;
if (`about -nt`) {
	$gTemplateColorSwatchOffset = 1;
} else {
	$gTemplateColorSwatchOffset = 2;
}

//	This global variable specifies the offset that should be used for 
//	offsetting option menus. This value will make an option menu button
//	align nicely with fields.
//
global int $gTemplateOptionMenuOffset;
if (`about -nt`) {
	$gTemplateOptionMenuOffset = 1;
} else {
	$gTemplateOptionMenuOffset = 0;
}

//	This global variable specifies the offset that should be used for 
//	offsetting check boxes. This value will make the check boxes
//	align nicely with fields.
//
global int $gTemplateCheckBoxOffset;
if (`about -nt`) {
	$gTemplateCheckBoxOffset = 1;
} else {
	$gTemplateCheckBoxOffset = 0;
}

// Original 
if ($kBigFont) {
	$kTextColumnWidthIndex = 170;
	$kSingleWidgetWidthIndex = 80;
	$kSecondColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
	$kThirdColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
	$kFourthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
	$kFifthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
	$kSliderWidgetWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
	$kUnitWidthIndex = 20;
	$kLongWordExtra = 12;
	$gStandardIdent = 5;
	$kOffsetText = 5;
	$kMapArrowIndex = 26;
	$kMapButtonIndex = ($kSingleWidgetWidthIndex - $kMapArrowIndex);
	$kScrollBar = 18;
	$kMagicSpacerNumber = 40;
	$gStandardWindowWidthIndex = ($gStandardIdent +
				$kTextColumnWidthIndex + 
				( 3 * $kSingleWidgetWidthIndex ) +
				$kUnitWidthIndex +
				$gStandardIdent +
				$kMagicSpacerNumber +
				$kScrollBar);
	$gStandardWidgetAreaIndex = ($gStandardWindowWidthIndex -
				($gStandardIdent +  
				$kTextColumnWidthIndex +
				$gStandardIdent + $kScrollBar));
	$gTextColumnWidthIndex = $kTextColumnWidthIndex;
	$gChannelBoxWidgetWidthIndex = $kTextColumnWidthIndex + 
				(3 * $kSingleWidgetWidthIndex);
	$gChannelBoxWidgetHeightIndex = 200;
} 


  //
  //  Declare we wish to make templates, first check if they exist else where
  //
	if (!`uiTemplate -exists DefaultTemplate`) {
		uiTemplate DefaultTemplate;
	}
	if (!`uiTemplate -exists ShaderTemplateItems`) {
		uiTemplate ShaderTemplateItems;
	}

// For internationalization

string $index = (uiRes("m_setDefaultTemplates.kIndex"));
string $notmapped = (uiRes("m_setDefaultTemplates.kNotMapped"));
string $map = (uiRes("m_setDefaultTemplates.kMap"));
string $mapping = (uiRes("m_setDefaultTemplates.kMapping"));
//////////////////////////////////////////////////////////////////////////
//									//
// 	Start of all  default templates 				//
//									//
//		DefaultTemplate						//
//									//
//////////////////////////////////////////////////////////////////////////
	$gDefaultTemplateLabelWidth        = $kTextColumnWidthIndex;
	$gDefaultTemplateLabelOffset       = $kOffsetText;
	$gDefaultTemplateSingleWidgetWidth = $kSingleWidgetWidthIndex;
	$gDefaultTemplateExtraWidth        = $kMapButtonIndex;

//	window -defineTemplate DefaultTemplate
//		-w $gStandardWindowWidthIndex
//		-h 550;

	columnLayout -defineTemplate DefaultTemplate
		-adj false -columnAttach "left" 0;

	scrollLayout -defineTemplate DefaultTemplate
		-verticalScrollBarThickness $kScrollBar;

	separator -defineTemplate DefaultTemplate
		-height 5;

	// Defines rowLayouts for single rows, two rows, & three rows
	rowLayout -defineTemplate DefaultTemplate 

		//	Assume a non-labeled item. It should be aligned with the
		//	control area.
		//
		-columnAlign1  "left"
		-columnAttach1 "left"
		-columnOffset1 ($kTextColumnWidthIndex)
		-columnWidth1  ($kTextColumnWidthIndex
					   + (3 * $kSingleWidgetWidthIndex))

		//	Assume a labeled item.
		//
		-columnAlign2  "right" "left"
		-columnAttach2 "both" "both"
		-columnOffset2 $kOffsetText 0
		-columnWidth2  $kTextColumnWidthIndex
					   (3 * $kSingleWidgetWidthIndex)

		//	Assume label, some control, and slider type format.
		//	
		-columnAlign3  "right" "left" "left"
		-columnAttach3 "both" "both" "both"
		-columnOffset3 $kOffsetText 0 0
		-columnWidth3  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSingleWidgetWidthIndex

		//	Assume label, and 3 controls type format.
		//
		-columnAlign4  "right" "left" "left" "left"
		-columnAttach4 "both" "both" "both" "both"
		-columnOffset4 $kOffsetText 0 0 0
		-columnWidth4  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSingleWidgetWidthIndex
					   $kSingleWidgetWidthIndex

		//	Assume label, 3 controls, and an extra label type format.
		//
		-columnAlign5  "right" "left" "left" "left" "left"
		-columnAttach5 "both" "both" "both" "both" "both"
		-columnOffset5 $kOffsetText 0 0 0 0
		-columnWidth5  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSingleWidgetWidthIndex
					   $kSingleWidgetWidthIndex
					   ($kMapButtonIndex + $kMapArrowIndex)

		//	Assume label, 3 controls, and two extra items.
		//
		-columnAlign6  "right" "left" "left" "left" "left" "left"
		-columnAttach6 "both" "both" "both" "both" "both" "both"
		-columnOffset6 $kOffsetText 0 0 0 0 0
		-columnWidth6  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSingleWidgetWidthIndex
					   $kSingleWidgetWidthIndex
					   $kMapButtonIndex 
					   $kMapArrowIndex
		;

	frameLayout -defineTemplate DefaultTemplate
		-backgroundShade true
		-mw $gStandardFrameLayoutMarginWidth
		-collapsable   true
		-collapse      true
		-labelIndent $gStandardIdent
		-font "boldLabelFont";

	tabLayout -defineTemplate DefaultTemplate
		-childResizable true;

	optionMenuGrp -defineTemplate DefaultTemplate
		-label        ""
		-columnAlign  1 "right"
		-columnAttach 1 "both" $kOffsetText
		-columnAlign  2 "left"
		-columnAttach 2 "both" $gTemplateOptionMenuOffset
	   	-columnWidth  1 $kTextColumnWidthIndex
		-columnWidth  3 ($kMapButtonIndex + $kMapArrowIndex);

	floatSliderGrp -defineTemplate DefaultTemplate
		-label      ""

		-minValue   0.0
		-maxValue   1.0 
		-precision  4
		-sliderStep 0.01 
		-value      0.0 
		-field      true

		-columnAlign3  "right" "left" "left"
		-columnAttach3 "both" "both" "both"
		-columnOffset3 $kOffsetText 0 0
		-columnWidth3  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSliderWidgetWidthIndex

		-columnAlign4  "right" "left" "left" "left"
		-columnAttach4 "both" "both" "both" "both"
		-columnOffset4 $kOffsetText 0 0 0
		-columnWidth4  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSliderWidgetWidthIndex
					   ($kMapButtonIndex + $kMapArrowIndex)
		;

	// Same as floatFieldGrp except they are used in
	// connect attribute commands
	attrFieldGrp -defineTemplate DefaultTemplate
		-label        ""
		-precision    4
		-columnAlign  1 "right"
		-columnAttach 1 "right" $kOffsetText
		-columnWidth3 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex
		-columnWidth4 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
		-columnWidth5 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
			($kMapButtonIndex + $kMapArrowIndex)
		-columnWidth6 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
			$kMapButtonIndex $kUnitWidthIndex;

	// Same as floatSliderGrp except they are used in
	// connect attribute commands
	attrFieldSliderGrp -defineTemplate DefaultTemplate
		-precision    4
		-sliderStep   0.001 
		-label        ""
		-columnAlign  1 "right"
		-columnAttach 1 "right" $kOffsetText
		-columnWidth1 $kTextColumnWidthIndex 
		-columnWidth2 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
		-columnWidth3 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
			$kSliderWidgetWidthIndex
		-columnWidth4 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
			$kSliderWidgetWidthIndex $kMapButtonIndex;

	attrColorSliderGrp -defineTemplate DefaultTemplate
		-label        ""
		-columnAlign4 "right" "left" "left" "center"
		-columnAttach4 "both" "both" "both" "both"
		-columnOffset4 $kOffsetText $gTemplateColorSwatchOffset 0 0
		-columnWidth4 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSliderWidgetWidthIndex $kMapButtonIndex
		;

	attrEnumOptionMenuGrp -defineTemplate DefaultTemplate
		-label         ""

		-columnAlign2  "right" "left"
		-columnAttach2 "both" "both"
		-columnOffset2 $kOffsetText $gTemplateOptionMenuOffset
		-columnWidth 1 $kTextColumnWidthIndex
		;

	intSliderGrp -defineTemplate DefaultTemplate
		-minValue     0
		-maxValue     100 
		-step         1
		-value        0
		-field        true
		-label        ""

		-columnAlign3  "right" "left" "left"
		-columnAttach3 "both" "both" "both"
		-columnOffset3 $kOffsetText 0 0
		-columnWidth3  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSliderWidgetWidthIndex

		-columnAlign4  "right" "left" "left" "left"
		-columnAttach4 "both" "both" "both" "both"
		-columnOffset4 $kOffsetText 0 0 0
		-columnWidth4  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSliderWidgetWidthIndex
					   ($kMapButtonIndex + $kMapArrowIndex)
		;

	intFieldGrp -defineTemplate DefaultTemplate
		-label        ""
		-nf 1
		-columnAlign  1 "right"
		-columnAttach 1 "right" $kOffsetText
		-columnWidth1 $kTextColumnWidthIndex
		-columnWidth2 $kTextColumnWidthIndex $kSingleWidgetWidthIndex
		-columnWidth3 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex
		-columnWidth4 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
		-columnWidth5 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
			($kMapButtonIndex + $kMapArrowIndex);

	floatFieldGrp -defineTemplate DefaultTemplate
		-label        ""
		-columnAlign  1 "right"
		-columnAttach 1 "right" $kOffsetText
		-precision    4
 		-columnWidth1 $kTextColumnWidthIndex 
		-columnWidth2 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
		-columnWidth3 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex
		-columnWidth4 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
		-columnWidth5 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex 
			($kMapButtonIndex + $kMapArrowIndex);

	colorSliderGrp -defineTemplate DefaultTemplate
		-label        ""
		-rgbValue     0 0 0 
		-columnAlign3 "right" "left" "left"
		-columnAttach3 "both" "both" "both"
		-columnOffset3 $kOffsetText $gTemplateColorSwatchOffset 0
		-columnWidth3 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSliderWidgetWidthIndex
		;

	colorIndexSliderGrp -defineTemplate DefaultTemplate
		-label        ""
		-extraLabel   $index
		-value        1 
		-columnAlign4  "right" "left" "left" "left"
		-columnAttach4 "both" "both" "both" "both"
		-columnOffset4 $kOffsetText $gTemplateColorSwatchOffset 0 0
		-columnWidth4  $kTextColumnWidthIndex
					   $kSingleWidgetWidthIndex 
					   $kSliderWidgetWidthIndex
					   ($kMapButtonIndex + $kMapArrowIndex)
		;

	colorSliderButtonGrp -defineTemplate DefaultTemplate
		-label               ""
		-rgbValue            0 0 0 
		-buttonLabel        $map
 		-symbolButtonDisplay true 
		-image               "navButtonConnected.png"
		-columnAlign5 "right" "left" "left" "left" "left"
		-columnAttach5 "both" "both" "both" "both" "both"
		-columnOffset5 $kOffsetText $gTemplateColorSwatchOffset 0 0 0
		-columnWidth5 $kTextColumnWidthIndex $kSingleWidgetWidthIndex 
			$kSliderWidgetWidthIndex $kMapButtonIndex $kMapArrowIndex
		;


	// This defines all the spaces between 1,2,3,4, radioButtons
	radioButtonGrp -defineTemplate DefaultTemplate
		-label        ""

		-columnAttach2 both left
		-columnAttach3 both both left 
		-columnAttach4 both both both left 
		-columnAttach5 both both both both left 

		-columnOffset2 $kOffsetText 0
		-columnOffset3 $kOffsetText 0 0
		-columnOffset4 $kOffsetText 0 0 0
		-columnOffset5 $kOffsetText 0 0 0 0

		-columnAlign2 right left 
		-columnAlign3 right left left 
		-columnAlign4 right left left left 
		-columnAlign5 right left left left left 

		-columnWidth 1 $kTextColumnWidthIndex 
		-columnWidth3 $kTextColumnWidthIndex 
		              ($kSecondColumnWidthIndex) ($kThirdColumnWidthIndex + $kLongWordExtra)
		-columnWidth4 $kTextColumnWidthIndex
		              ($kSingleWidgetWidthIndex) ($kSingleWidgetWidthIndex) 
		              ($kFourthColumnWidthIndex + $kThreeButtonRadioExtra)
		-columnWidth5 $kTextColumnWidthIndex 
		              $kSingleWidgetWidthIndex $kSingleWidgetWidthIndex
					  $kSingleWidgetWidthIndex 
		             ($kFifthColumnWidthIndex + $kLongWordExtra);

	checkBoxGrp -defineTemplate DefaultTemplate
		-label         ""

		-columnAttach2 both left
		-columnAttach3 both both left 
		-columnAttach4 both both both left 
		-columnAttach5 both both both both left 

		-columnOffset2 $kOffsetText $gTemplateCheckBoxOffset
		-columnOffset3 $kOffsetText $gTemplateCheckBoxOffset
		               $gTemplateCheckBoxOffset
		-columnOffset4 $kOffsetText $gTemplateCheckBoxOffset
		               $gTemplateCheckBoxOffset $gTemplateCheckBoxOffset
		-columnOffset5 $kOffsetText $gTemplateCheckBoxOffset
		               $gTemplateCheckBoxOffset $gTemplateCheckBoxOffset
		               $gTemplateCheckBoxOffset

		-columnAlign2 right left 
		-columnAlign3 right left left 
		-columnAlign4 right left left left 
		-columnAlign5 right left left left left 

		-columnWidth 1 $kTextColumnWidthIndex 
		-columnWidth3 $kTextColumnWidthIndex 
			($kSecondColumnWidthIndex) ($kThirdColumnWidthIndex + $kLongWordExtra)
		-columnWidth4 $kTextColumnWidthIndex 
			($kSingleWidgetWidthIndex) ($kSingleWidgetWidthIndex) 
			($kFourthColumnWidthIndex + $kLongWordExtra)
		-columnWidth5 $kTextColumnWidthIndex 
			$kSingleWidgetWidthIndex $kSingleWidgetWidthIndex 
			$kSingleWidgetWidthIndex ($kFifthColumnWidthIndex + $kLongWordExtra)
		;

	textFieldGrp -defineTemplate DefaultTemplate
		-label        ""
		-adjustableColumn 2
		-columnAlign  1 "right"
		-columnAttach 1 "right" $kOffsetText
		-columnAttach 2 "both" 0
	    -columnWidth1 $kTextColumnWidthIndex 
	    -columnWidth2 $kTextColumnWidthIndex 
				( 3 * $kSingleWidgetWidthIndex );

	textFieldButtonGrp -defineTemplate DefaultTemplate
		-label        ""
		-adjustableColumn 2
		-text         $notmapped 
