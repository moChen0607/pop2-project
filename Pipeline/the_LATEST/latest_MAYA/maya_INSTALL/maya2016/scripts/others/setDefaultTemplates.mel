//////////////////////////////////////////////////////////////////////////
//
// 	New tool option box template
//
//////////////////////////////////////////////////////////////////////////

// Global variables to define frame spacing and description indents
//
global int $gToolOptionBoxTemplateDescriptionMarginWidth 	= 20;
global int $gToolOptionBoxTemplateFrameSpacing				= 4;
global int $gToolOptionBoxTemplateFrameMarginHeight			= 4;
global int $gToolOptionBoxTemplateFrameMarginWidth			= 0;

global int $gToolOptionBoxTemplateInnerFrameMarginHeight;
global int $gToolOptionBoxTemplateInnerFrameMarginWidth;

$gToolOptionBoxTemplateInnerFrameMarginHeight = 
$gToolOptionBoxTemplateFrameSpacing - $gToolOptionBoxTemplateFrameMarginHeight;
if( $gToolOptionBoxTemplateInnerFrameMarginHeight < 0 )
	$gToolOptionBoxTemplateInnerFrameMarginHeight = 0;

$gToolOptionBoxTemplateInnerFrameMarginWidth = 
$gToolOptionBoxTemplateFrameSpacing - $gToolOptionBoxTemplateFrameMarginWidth;
if( $gToolOptionBoxTemplateInnerFrameMarginWidth < 0 )
	$gToolOptionBoxTemplateInnerFrameMarginWidth = 0;

//	Amount of space for the label.
//
//	Feel free to modify this value to alter the amount of space
//	allocated for the group label.
//
//	NOTE: You can also reference this value outside of this script
//	if you have some control group that is not part of the template.
//	But be sure to ask yourself why you are using a non-standard group.
//	Perhaps it should be added to the template or you should be using
//	one from the template.
//
global int $gToolOptionBoxTemplateLabelWidth         = 132;

//	Amount of space between label and the control directly to the right
//	of it.
//
//	Feel free to modify this value or reference it in other scripts.
//
//	NOTE: However, please see the note above.
//
global int $gToolOptionBoxTemplateLabelOffset        = 5;

//	Amount of space for controls, eg. all sliders
//	fields, check boxes, and buttons must combine to a width
//	equal to this value.
//
//	Feel free to modify this value or reference it in other scripts.
//
global int $gToolOptionBoxTemplateControlAreaWidth   = 186;

//	Calculate the size of fields, sliders and swatches based on the
//	above value.
//
//	NOTE: You really shouldn't be changing these values as these look
//	best to keep fields, swatches and sliders aligned.
//
int $kToolOptionBoxFieldWidth  = $gToolOptionBoxTemplateControlAreaWidth / 3;
int $kToolOptionBoxSwatchWidth = $kToolOptionBoxFieldWidth;
int $kToolOptionBoxSliderWidth = 2 * $kToolOptionBoxFieldWidth;

//	Amount of space for extra button controls that appear on the far
//	right of control groups. The values should be big enough to accommodate
//	buttons with text labels and image labels. Their combined width will
//	most likely be big enough to accommodate extra unit labels.
//
//	NOTE: you really shouldn't be changing these two values unless you
//	are changing the image pixmaps or are altering the text appearing
//	in the buttons.
//
$kTextButtonWidth  = 38;  // Size of a button with text label.
$kImageButtonWidth = 25;  // Size of a button with image label.
//
//	The following value should at least equal the sum of the previous two.
//
global int $gToolOptionBoxTemplateExtraWidth         = 63;

//
//	For extra labels also specify an offset.
//
global int $gToolOptionBoxTemplateExtraLabelOffset   = 0;

// Create ToolOptionBoxTemplate
//
if (!`uiTemplate -exists ToolOptionBoxTemplate`) {
	uiTemplate ToolOptionBoxTemplate;
}

columnLayout -defineTemplate ToolOptionBoxTemplate
	-adj true 
	-columnAttach "left" 0;

scrollLayout -defineTemplate ToolOptionBoxTemplate
	-verticalScrollBarThickness $kScrollBar
	-horizontalScrollBarThickness 0
	-childResizable 1;

separator -defineTemplate ToolOptionBoxTemplate
	-style      "none"
	-height     2 
	-horizontal true
	;

frameLayout -defineTemplate ToolOptionBoxTemplate
	-backgroundShade true 
	-collapsable   false
	-collapse      false
	-label 		""
	-labelIndent $gStandardIdent
	-font "boldLabelFont"
	-marginHeight  $gToolOptionBoxTemplateFrameMarginHeight
	-marginWidth  $gToolOptionBoxTemplateFrameMarginWidth
	;
	
text -defineTemplate ToolOptionBoxTemplate
	-align	"left"
	;

rowLayout -defineTemplate ToolOptionBoxTemplate 

	//	Assume a non-labeled item. It should be aligned with the
	//	control area.
	//
	-columnAlign1  "left"
	-columnAttach1 "left"
	-columnOffset1 ($gToolOptionBoxTemplateLabelWidth)
	-columnWidth1  ($gToolOptionBoxTemplateLabelWidth
				   + $gToolOptionBoxTemplateControlAreaWidth)

	//	Assume a labeled item.
	//
	-columnAlign2  "right" "left"
	-columnAttach2 "both" "both"
	-columnOffset2 $gToolOptionBoxTemplateLabelOffset 0
	-columnWidth2  $gToolOptionBoxTemplateLabelWidth
				   $gToolOptionBoxTemplateControlAreaWidth

	//	Assume label, some control, and slider type format.
	//	
	-columnAlign3  "right" "left" "left"
	-columnAttach3 "both" "both" "both"
	-columnOffset3 $gToolOptionBoxTemplateLabelOffset 0 0
	-columnWidth3  $gToolOptionBoxTemplateLabelWidth
				   $kToolOptionBoxFieldWidth 
				   $kToolOptionBoxSliderWidth

	//	Assume label, and 3 controls type format.
	//
	-columnAlign4  "right" "left" "left" "left"
	-columnAttach4 "both" "both" "both" "both"
	-columnOffset4 $gToolOptionBoxTemplateLabelOffset 0 0 0
	-columnWidth4  $gToolOptionBoxTemplateLabelWidth
				   $kToolOptionBoxFieldWidth 
				   $kToolOptionBoxFieldWidth
				   $kToolOptionBoxFieldWidth

	//	Assume label, 3 controls, and an extra label type format.
	//
	-columnAlign5  "right" "left" "left" "left" "left"
	-columnAttach5 "both" "both" "both" "both" "both"
	-columnOffset5 $gToolOptionBoxTemplateLabelOffset 0 0 0 $gToolOptionBoxTemplateExtraLabelOffset
	-columnWidth5  $gToolOptionBoxTemplateLabelWidth
				   $kToolOptionBoxFieldWidth 
				   $kToolOptionBoxFieldWidth
				   $kToolOptionBoxFieldWidth
				   $gToolOptionBoxTemplateExtraWidth

	//	Assume label, 3 controls, and two extra items.
	//
	-columnAlign6  "right" "left" "left" "left" "left" "left"
	-columnAttach6 "both" "both" "both" "both" "both" "both"
	-columnOffset6 $gToolOptionBoxTemplateLabelOffset 0 0 0 0 $gToolOptionBoxTemplateExtraLabelOffset
	-columnWidth6  $gToolOptionBoxTemplateLabelWidth
				   $kToolOptionBoxFieldWidth 
				   $kToolOptionBoxFieldWidth
				   $kToolOptionBoxFieldWidth
				   $kTextButtonWidth 
				   $kImageButtonWidth
	;

