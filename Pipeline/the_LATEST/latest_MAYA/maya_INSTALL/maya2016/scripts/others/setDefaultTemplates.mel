//////////////////////////////////////////////////////////////////////////
//
// 	Create a spiffy new template that should be used for all new
//	option boxes and tool settings.
//
//	This one is much better at spacing the controls properly and
//	is a lot easier to figure out what values to change to alter
//	the layout.
//
//  OUT OF DATE: Use OptionBoxTemplate or ToolOptionBoxTemplate below 
//
//////////////////////////////////////////////////////////////////////////

if (!`uiTemplate -exists OptionsTemplate`) {
	uiTemplate OptionsTemplate;
}

//	Amount of space for the label.
//
//	Feel free to modify this value to alter the amount of space
//	allocated for the group label.
//
//	NOTE: You can also reference this value outside of this script
//	if you have some control group that is not part of the template.
//	But be sure to ask yourself why you are using a non-standard group.
//	Perhaps it should be added to the template or you should be using
//	one from the template.
//
global int $gOptionsTemplateLabelWidth         = 132;

//	Amount of space between label and the control directly to the right
//	of it.
//
//	Feel free to modify this value or reference it in other scripts.
//
//	NOTE: However, please see the note above.
//
global int $gOptionsTemplateLabelOffset        = 5;

//	Amount of space for controls, eg. all sliders
//	fields, check boxes, and buttons must combine to a width
//	equal to this value.
//
//	Feel free to modify this value or reference it in other scripts.
//
// 
global int $gOptionsTemplateControlAreaWidth   = 240;
if (!`about -uiLanguageIsLocalized`) {
	// english version is okay with original width (186)
	$gOptionsTemplateControlAreaWidth = 186;
}

//	Calculate the size of fields, sliders and swatches based on the
//	above value.
//
//	NOTE: You really shouldn't be changing these values as these look
//	best to keep fields, swatches and sliders aligned.
//
int $kFieldWidth  = $gOptionsTemplateControlAreaWidth / 3;
int $kSwatchWidth = $kFieldWidth;
int $kSliderWidth = 2 * $kFieldWidth;

//	Amount of space for extra button controls that appear on the far
//	right of control groups. The values should be big enough to accommodate
//	buttons with text labels and image labels. Their combined width will
//	most likely be big enough to accommodate extra unit labels.
//
//	NOTE: you really shouldn't be changing these two values unless you
//	are changing the image pixmaps or are altering the text appearing
//	in the buttons.
//
int $kTextButtonWidth  = 38;  // Size of a button with text label.
int $kImageButtonWidth = 25;  // Size of a button with image label.
//
//	The following value should equal the sum of the previous two.
//
global int $gOptionsTemplateExtraWidth         = 63;

//
//	For extra labels also specify an offset.
//
global int $gOptionsTemplateExtraLabelOffset   = 0;

separator -defineTemplate OptionsTemplate
	-style      "none"
	-height     2 
	-horizontal true
	;

frameLayout -defineTemplate OptionsTemplate
	-backgroundShade true 
	-mw $gStandardFrameLayoutMarginWidth
	-collapsable   true
	-collapse      true
	-labelIndent   $gStandardIdent
	-font          "boldLabelFont";

columnLayout -defineTemplate OptionsTemplate
	-adj true 
	-columnAttach "left" 0;

rowLayout -defineTemplate OptionsTemplate 

	//	Assume a non-labeled item. It should be aligned with the
	//	control area.
	//
	-columnAlign1  "left"
	-columnAttach1 "left"
	-columnOffset1 ($gOptionsTemplateLabelWidth)
	-columnWidth1  ($gOptionsTemplateLabelWidth
				   + $gOptionsTemplateControlAreaWidth)

	//	Assume a labeled item.
	//
	-columnAlign2  "right" "left"
	-columnAttach2 "both" "both"
	-columnOffset2 $gOptionsTemplateLabelOffset 0
	-columnWidth2  $gOptionsTemplateLabelWidth
				   $gOptionsTemplateControlAreaWidth

	//	Assume label, some control, and slider type format.
	//	
	-columnAlign3  "right" "left" "left"
	-columnAttach3 "both" "both" "both"
	-columnOffset3 $gOptionsTemplateLabelOffset 0 0
	-columnWidth3  $gOptionsTemplateLabelWidth
				   $kFieldWidth 
				   $kSliderWidth

	//	Assume label, and 3 controls type format.
	//
	-columnAlign4  "right" "left" "left" "left"
	-columnAttach4 "both" "both" "both" "both"
	-columnOffset4 $gOptionsTemplateLabelOffset 0 0 0
	-columnWidth4  $gOptionsTemplateLabelWidth
				   $kFieldWidth 
				   $kFieldWidth
				   $kFieldWidth

	//	Assume label, 3 controls, and an extra label type format.
	//
	-columnAlign5  "right" "left" "left" "left" "left"
	-columnAttach5 "both" "both" "both" "both" "both"
	-columnOffset5 $gOptionsTemplateLabelOffset 0 0 0 $gOptionsTemplateExtraLabelOffset
	-columnWidth5  $gOptionsTemplateLabelWidth
				   $kFieldWidth 
				   $kFieldWidth
				   $kFieldWidth
				   $gOptionsTemplateExtraWidth

	//	Assume label, 3 controls, and two extra items.
