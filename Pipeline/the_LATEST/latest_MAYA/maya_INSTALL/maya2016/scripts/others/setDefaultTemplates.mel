//////////////////////////////////////////////////////////////////////////
//
// 	New option box template 
//
//////////////////////////////////////////////////////////////////////////

$kTextColumnWidthIndex = 145;
$kSingleWidgetWidthIndex = 70;
$kSecondColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
$kThirdColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
$kFourthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
$kFifthColumnWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
$kSliderWidgetWidthIndex = ( 2 * $kSingleWidgetWidthIndex );
$kUnitWidthIndex = 20;
$kLongWordExtra = 10;
$gStandardIdent = 5;
$kOffsetText = 5;
$kMapArrowIndex = 44;
$kMapButtonIndex = ($kSingleWidgetWidthIndex - $kMapArrowIndex);
$kScrollBar = 16;
$kMagicSpacerNumber = 40;
$gStandardWindowWidthIndex = ($gStandardIdent +
			$kTextColumnWidthIndex + 
			( 3 * $kSingleWidgetWidthIndex ) +
			$kUnitWidthIndex +
			$gStandardIdent +
			$kMagicSpacerNumber +
			$kScrollBar);
$gStandardWidgetAreaIndex = ($gStandardWindowWidthIndex -
			($gStandardIdent +  
			$kTextColumnWidthIndex +
			$gStandardIdent + $kScrollBar));
$gTextColumnWidthIndex = $kTextColumnWidthIndex;

// Global variables to define frame spacing and description indents
//
global int $gOptionBoxTemplateDescriptionMarginWidth 	= 20;
global int $gOptionBoxTemplateFrameSpacing				= 4;
global int $gOptionBoxTemplateFrameMarginHeight			= 4;
global int $gOptionBoxTemplateFrameMarginWidth			= 0;

global int $gOptionBoxTemplateInnerFrameMarginHeight;
global int $gOptionBoxTemplateInnerFrameMarginWidth;

$gOptionBoxTemplateInnerFrameMarginHeight = $gOptionBoxTemplateFrameSpacing - $gOptionBoxTemplateFrameMarginHeight;
if( $gOptionBoxTemplateInnerFrameMarginHeight < 0 )
	$gOptionBoxTemplateInnerFrameMarginHeight = 0;
	
$gOptionBoxTemplateInnerFrameMarginWidth = $gOptionBoxTemplateFrameSpacing - $gOptionBoxTemplateFrameMarginWidth;
if( $gOptionBoxTemplateInnerFrameMarginWidth < 0 )
	$gOptionBoxTemplateInnerFrameMarginWidth = 0;
	
// Global variables to define widget spacing
//
global int $gOptionBoxTemplateTextColumnWidth;
global int $gOptionBoxTemplateSingleWidgetWidth;
global int $gOptionBoxTemplateSliderWidgetWidth;
global int $gOptionBoxTemplateOffsetText;

$gOptionBoxTemplateTextColumnWidth = $kTextColumnWidthIndex;
$gOptionBoxTemplateSingleWidgetWidth = $kSingleWidgetWidthIndex;
$gOptionBoxTemplateSliderWidgetWidth = $kSliderWidgetWidthIndex;
$gOptionBoxTemplateOffsetText = $kOffsetText;

// Create OptionBoxTemplate
//
if (!`uiTemplate -exists OptionBoxTemplate`) {
	uiTemplate OptionBoxTemplate;
}

columnLayout -defineTemplate OptionBoxTemplate
	-adj true 
	-columnAttach "left" 0;

scrollLayout -defineTemplate OptionBoxTemplate
	-verticalScrollBarThickness $kScrollBar
	-horizontalScrollBarThickness 0
	-childResizable 1;

separator -defineTemplate OptionBoxTemplate
	-style      "none"
	-height     2 
	;

// Defines rowLayouts for single rows, two rows, & three rows
rowLayout -defineTemplate OptionBoxTemplate 

	//	Assume a non-labeled item. It should be aligned with the
	//	control area.
	//
	-columnAlign1  "left"
	-columnAttach1 "left"
	-columnOffset1 ($kTextColumnWidthIndex)
	-columnWidth1  ($kTextColumnWidthIndex
				   + (3 * $kSingleWidgetWidthIndex))

	//	Assume a labeled item.
	//
	-columnAlign2  "right" "left"
	-columnAttach2 "both" "both"
	-columnOffset2 $kOffsetText 0
	-columnWidth2  $kTextColumnWidthIndex
				   (3 * $kSingleWidgetWidthIndex)

	//	Assume label, some control, and slider type format.
	//	
	-columnAlign3  "right" "left" "left"
	-columnAttach3 "both" "both" "both"
	-columnOffset3 $kOffsetText 0 0
	-columnWidth3  $kTextColumnWidthIndex
				   $kSingleWidgetWidthIndex 
				   $kSingleWidgetWidthIndex

	//	Assume label, and 3 controls type format.
	//
	-columnAlign4  "right" "left" "left" "left"
	-columnAttach4 "both" "both" "both" "both"
	-columnOffset4 $kOffsetText 0 0 0
	-columnWidth4  $kTextColumnWidthIndex
				   $kSingleWidgetWidthIndex 
				   $kSingleWidgetWidthIndex
				   $kSingleWidgetWidthIndex

	//	Assume label, 3 controls, and an extra label type format.
	//
	-columnAlign5  "right" "left" "left" "left" "left"
	-columnAttach5 "both" "both" "both" "both" "both"
	-columnOffset5 $kOffsetText 0 0 0 0
	-columnWidth5  $kTextColumnWidthIndex
				   $kSingleWidgetWidthIndex 
				   $kSingleWidgetWidthIndex
				   $kSingleWidgetWidthIndex
				   ($kMapButtonIndex + $kMapArrowIndex)

	//	Assume label, 3 controls, and two extra items.
	//
	-columnAlign6  "right" "left" "left" "left" "left" "left"
	-columnAttach6 "both" "both" "both" "both" "both" "both"
	-columnOffset6 $kOffsetText 0 0 0 0 0
	-columnWidth6  $kTextColumnWidthIndex
				   $kSingleWidgetWidthIndex 
				   $kSingleWidgetWidthIndex
				   $kSingleWidgetWidthIndex
				   $kMapButtonIndex 
				   $kMapArrowIndex
	;

