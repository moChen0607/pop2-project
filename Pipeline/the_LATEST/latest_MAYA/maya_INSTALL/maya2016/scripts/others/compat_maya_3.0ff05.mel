//
// Global Maya compatibility file, version 3.0ff05.
//
////////////////////////////////////////////////////////////////////////////
//
// This version becomes effective as of the December 1, 1999 release.
//
//  PLEASE make sure you're familiar with
//  http://w3.tor.aw.sgi.com/maya/swotff/docs/nodeChanges.html
//  In particular:
//  - make sure to add entries for the renaming of both long
//    and short attribute names
//    BUT: do NOT add any entries when renaming attributes due to name clashes
//    (there is no way to tell WHICH of the clashing names is changing)
//  - make sure to add entries for the removal of both long
//    and short attribute names for attributes you're removing
//  - make sure to change any Mel scripts that refer to your
//    attribute by name; only Maya ASCII (.ma) and Binary (.mb) files
//    get things automatically renamed on file loading.
//  - do not attempt to re-name any node binary TAGS, as it is not currently
//    supported.
//
////////////////////////////////////////////////////////////////////////////

attrCompatibility -v "3.0ff05";

attrCompatibility -renameAttr uvSetName polyTweakUV mapSet;
attrCompatibility -renameAttr uvs polyTweakUV ms;
attrCompatibility -renameAttr uvSetName polyPlanProj mapSet;
attrCompatibility -renameAttr uvs polyPlanProj ms;
attrCompatibility -renameAttr uvSetName polySphProj mapSet;
attrCompatibility -renameAttr uvs polySphProj ms;
attrCompatibility -renameAttr uvSetName polyCylProj mapSet;
attrCompatibility -renameAttr uvs polyCylProj ms;
attrCompatibility -renameAttr uvSetName polyPrimitive mapSet;
attrCompatibility -renameAttr uvs polyPrimitive ms;
attrCompatibility -renameAttr uvSetName polyCreateFace mapSet;
attrCompatibility -renameAttr uvs polyCreateFace ms;
attrCompatibility -renameAttr uvSetName polyMapCut mapSet;
attrCompatibility -renameAttr uvs polyMapCut ms;
attrCompatibility -renameAttr uvSetName polyMapDel mapSet;
attrCompatibility -renameAttr uvs polyMapDel ms;
attrCompatibility -renameAttr uvSetName polyMapSew mapSet;
attrCompatibility -renameAttr uvs polyMapSew ms;

// Rename old blindData base node, which is now obsolete anyway
attrCompatibility -nr oldBlindDataBase blindData;

// Change turbulence field "phase" to "phaseZ"
//
attrCompatibility -r phaseZ turbulenceField phase;
attrCompatibility -r phz turbulenceField phs;

// Renamed subdivision attributes on polyCylinder to be more intuitive
attrCompatibility -renameAttr sa polyCylinder sx;
attrCompatibility -renameAttr subdivisionsAxis polyCylinder subdivisionsX;
attrCompatibility -renameAttr sh polyCylinder sy;
attrCompatibility -renameAttr subdivisionsHeight polyCylinder subdivisionsY;
attrCompatibility -renameAttr sc polyCylinder sz;
attrCompatibility -renameAttr subdivisionsCaps polyCylinder subdivisionsZ;

// getting rid of references to "facet" in the UI:
attrCompatibility -nodeRename polyExtrudeFace polyExtrudeFacet;
attrCompatibility -renameAttr keepFacesTogether polyExtrudeFace keepFacetTogether;
attrCompatibility -renameAttr keepFacesTogether polyChipOff keepFacetTogether;

attrCompatibility -nodeRename polyCreateFace polyCreateFacet;
attrCompatibility -nodeRename polyMergeFace polyMergeFacet;
attrCompatibility -nodeRename polyMoveFace polyMoveFacet;
attrCompatibility -nodeRename polySmoothFace polySmoothFacet;
attrCompatibility -nodeRename polySubdFace polySubdFacet;

attrCompatibility -renameAttr dnp mesh dwf;
attrCompatibility -renameAttr displayNonPlanar mesh displayWarpFacet;
