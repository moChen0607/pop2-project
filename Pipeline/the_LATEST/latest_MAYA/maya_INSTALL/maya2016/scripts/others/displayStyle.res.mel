displayString -replace -value "Unknown Flag: ^1s" m_displayStyle.kDisplayStyleError;
displayString -replace -value "Usage: displayStyle FLAG\n" m_displayStyle.kDisplayStyleHelp0;
displayString -replace -value "       Changes the current display style in the active window.\n" m_displayStyle.kDisplayStyleHelp1;
displayString -replace -value "       -rgb/-rgbMode               = Use RGB mode\n" m_displayStyle.kDisplayStyleHelp10;
displayString -replace -value "       -ci/-colorIndex             = Use color index mode\n" m_displayStyle.kDisplayStyleHelp11;
displayString -replace -value "       -h/-help                    = Show this help information\n" m_displayStyle.kDisplayStyleHelp2;
displayString -replace -value "       -tx/-textured               = Use textured display\n" m_displayStyle.kDisplayStyleHelp3;
displayString -replace -value "       -ss/-smoothshaded           = Use smooth shaded display\n" m_displayStyle.kDisplayStyleHelp4;
displayString -replace -value "       -w/-wireframe               = Use wireframe display\n" m_displayStyle.kDisplayStyleHelp5;
displayString -replace -value "       -wos/-wireframeOnShaded     = Use wireframe-on-shaded display\n" m_displayStyle.kDisplayStyleHelp6;
displayString -replace -value "       -nowos/-noWireframeOnShaded = Use wireframe-on-shaded display\n" m_displayStyle.kDisplayStyleHelp7;
displayString -replace -value "       -db/-doubleBuffer           = Use double buffer mode\n" m_displayStyle.kDisplayStyleHelp8;
displayString -replace -value "       -sb/-singleBuffer           = Use single buffer mode\n" m_displayStyle.kDisplayStyleHelp9;
