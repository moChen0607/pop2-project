    menuItem
        -label (uiRes("m_menu_PA_Style_RMB.kToggleShade")) 
        -divider 0
        -subMenu 0
        -command ( "if ( `getPanel -to ( eval( \"getPanel -withFocus\" ) )` ) {" +
	               "   $currentPanel = `getPanel -withFocus`; " +
	               "   if (\"smoothShaded\"==`modelEditor -q -da $currentPanel`)" +  
	               "       modelEditor -e -da \"wireframe\" $currentPanel; " +
	               "   else" +
	               "       modelEditor -e -da \"smoothShaded\" $currentPanel;" +
	               "}" ) 
        -altModifier 0
        -ctrlModifier 0
        -shiftModifier 0
        -optionBox 0
        -enable 1
        -data 0
        -radialPosition "S" 
        -enableCommandRepeat 1
        -image "commandButton.png" 
