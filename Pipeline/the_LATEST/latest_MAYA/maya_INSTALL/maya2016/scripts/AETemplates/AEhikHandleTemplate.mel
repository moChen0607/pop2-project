		editorTemplate -suppress "dTwistRamp";
		editorTemplate -suppress "dTwistRampMult";
		editorTemplate -suppress "ikFkManipulation";
		editorTemplate -suppress "dTwistControlEnable";
		editorTemplate -suppress "inCurve";
		editorTemplate -suppress "renderLayerInfo";
		editorTemplate -suppress "stancePoseMatrix";
		editorTemplate -suppress "usingMB55Rig";
		editorTemplate -suppress "extra";
		editorTemplate -suppress "stiffness";
		editorTemplate -suppress "killPitch";
		editorTemplate -suppress "rollExtraction";
		editorTemplate -suppress "solving";
		editorTemplate -suppress "floorContacts";
