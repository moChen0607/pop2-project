global proc AEhikExpertModeEnable(string $nodeName )
{
	int $enable = `getAttr ($nodeName+".expertMode")`;
	editorTemplate -dimControl $nodeName "headPull" (! $enable);
	editorTemplate -dimControl $nodeName "hipsPull" (! $enable);
	editorTemplate -dimControl $nodeName "chestPull" (! $enable);
	editorTemplate -dimControl $nodeName "pullIterationCount" (! $enable);
	editorTemplate -dimControl $nodeName "leftElbowPull" (! $enable);
	editorTemplate -dimControl $nodeName "leftHandPullChest" (! $enable);
	editorTemplate -dimControl $nodeName "leftHandPullHips" (! $enable);
	editorTemplate -dimControl $nodeName "leftFingerBasePull" (! $enable);
	editorTemplate -dimControl $nodeName "rightElbowPull" (! $enable);
	editorTemplate -dimControl $nodeName "rightHandPullChest" (! $enable);
	editorTemplate -dimControl $nodeName "rightHandPullHips" (! $enable);
	editorTemplate -dimControl $nodeName "rightFingerBasePull" (! $enable);
	editorTemplate -dimControl $nodeName "leftKneePull" (! $enable);
	editorTemplate -dimControl $nodeName "leftFootPull" (! $enable);
	editorTemplate -dimControl $nodeName "leftToeBasePull" (! $enable);
	editorTemplate -dimControl $nodeName "rightKneePull" (! $enable);
	editorTemplate -dimControl $nodeName "rightFootPull" (! $enable);
	editorTemplate -dimControl $nodeName "rightToeBasePull" (! $enable);
	editorTemplate -dimControl $nodeName "neckStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "chestStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "spineStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "hipsStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "hipsEnforceGravity" (! $enable);
	editorTemplate -dimControl $nodeName "leftShoulderStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "leftArmStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "leftElbowMaxExtension" (! $enable);
	editorTemplate -dimControl $nodeName "leftElbowCompressionFactor" (! $enable);
	editorTemplate -dimControl $nodeName "rightShoulderStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "rightArmStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "rightElbowMaxExtension" (! $enable);
	editorTemplate -dimControl $nodeName "rightElbowCompressionFactor" (! $enable);
	editorTemplate -dimControl $nodeName "leftKneeCompressionFactor" (! $enable);
	editorTemplate -dimControl $nodeName "leftKneeMaxExtension" (! $enable);
	editorTemplate -dimControl $nodeName "leftLegStiffness" (! $enable);
	editorTemplate -dimControl $nodeName "rightKneeCompressionFactor" (! $enable);
	editorTemplate -dimControl $nodeName "rightKneeMaxExtension" (! $enable);
	editorTemplate -dimControl $nodeName "rightLegStiffness" (! $enable);
}

global proc AEhikHandleTemplate (string $nodeName )
{
	editorTemplate -beginScrollLayout;

		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kHIKHandleAttributes"))  -collapse 0;

		editorTemplate -addControl "activate";

		editorTemplate -addControl "postureType";
		editorTemplate -addControl "hipTranslationMode";
		editorTemplate -addControl "realisticShoulderSolving";
		editorTemplate -addControl "solveFingers";		

		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kPull"))  -collapse 1;
		editorTemplate -addControl "expertMode" "AEhikExpertModeEnable";
		editorTemplate -addControl "headPull";
		editorTemplate -addControl "chestPull";
		editorTemplate -addControl "hipsPull";
		editorTemplate -addControl "pullIterationCount";
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kLeftArmPull"))  -collapse 1;
		editorTemplate -addControl "leftElbowPull";
		editorTemplate -addControl "leftHandPullChest";
		editorTemplate -addControl "leftHandPullHips";
		editorTemplate -addControl "leftFingerBasePull";
		editorTemplate -endLayout;		
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kRightArmPull"))  -collapse 1;
		editorTemplate -addControl "rightElbowPull";
		editorTemplate -addControl "rightHandPullChest";
		editorTemplate -addControl "rightHandPullHips";
		editorTemplate -addControl "rightFingerBasePull";
		editorTemplate -endLayout;		
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kLeftLegPull"))  -collapse 1;
		editorTemplate -addControl "leftKneePull";
		editorTemplate -addControl "leftFootPull";
		editorTemplate -addControl "leftToeBasePull";
		editorTemplate -endLayout;
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kRightLegPull"))  -collapse 1;
		editorTemplate -addControl "rightKneePull";
		editorTemplate -addControl "rightFootPull";
		editorTemplate -addControl "rightToeBasePull";
		editorTemplate -endLayout;		

		editorTemplate -endLayout;		// end pull attrs.

		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kRollAttributes"))  -collapse 1;
		editorTemplate -addControl "rollExtractionMode";
		editorTemplate -addControl "leftArmRollMode";
		editorTemplate -addControl "leftArmRoll";
		editorTemplate -addControl "leftForeArmRollMode";
		editorTemplate -addControl "leftForeArmRoll";
		editorTemplate -addControl "rightArmRollMode";
		editorTemplate -addControl "rightArmRoll";
		editorTemplate -addControl "rightForeArmRollMode";
		editorTemplate -addControl "rightForeArmRoll";
		editorTemplate -addControl "leftUpLegRollMode";
		editorTemplate -addControl "leftUpLegRoll";
		editorTemplate -addControl "leftLegRollMode";
		editorTemplate -addControl "leftLegRoll";
		editorTemplate -addControl "rightUpLegRollMode";
		editorTemplate -addControl "rightUpLegRoll";
		editorTemplate -addControl "rightLegRollMode";
		editorTemplate -addControl "rightLegRoll";
		editorTemplate -endLayout;		// end roll attributes
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kStiffnessAttributes"))  -collapse 1;
		editorTemplate -addControl "neckStiffness";
		editorTemplate -addControl "chestStiffness";
		editorTemplate -addControl "spineStiffness";
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kLeftArmStiffness"))  -collapse 1;
		editorTemplate -addControl "leftShoulderStiffness";
		editorTemplate -addControl "leftArmStiffness";
		editorTemplate -addControl "leftElbowMaxExtension";
		editorTemplate -addControl "leftElbowCompressionFactor";
		editorTemplate -endLayout;
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kRightArmStiffness"))  -collapse 1;
		editorTemplate -addControl "rightShoulderStiffness";
		editorTemplate -addControl "rightArmStiffness";
		editorTemplate -addControl "rightElbowMaxExtension";		
		editorTemplate -addControl "rightElbowCompressionFactor";
		editorTemplate -endLayout;
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kHipsStiffness"))  -collapse 1;			
		editorTemplate -addControl "hipsStiffness";							
		editorTemplate -addControl "hipsEnforceGravity";
		editorTemplate -endLayout;
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kLeftLegStiffness"))  -collapse 1;		
		editorTemplate -addControl "leftLegStiffness";		
		editorTemplate -addControl "leftKneeMaxExtension";
		editorTemplate -addControl "leftKneeCompressionFactor";
		editorTemplate -endLayout;
		
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kRightLegStiffness"))  -collapse 1;		
		editorTemplate -addControl "rightLegStiffness";	
		editorTemplate -addControl "rightKneeMaxExtension";
		editorTemplate -addControl "rightKneeCompressionFactor";
		editorTemplate -endLayout;	

		editorTemplate -endLayout;	// end Stiffness attr	

		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kKillPitchAttributes"))  -collapse 1;
		editorTemplate -addControl "leftElbowKillPitch";
		editorTemplate -addControl "rightElbowKillPitch";		
		editorTemplate -addControl "leftKneeKillPitch";
		editorTemplate -addControl "rightKneeKillPitch";		
		editorTemplate -endLayout;	

		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kFingerTipSizes"))  -collapse 1; 
		editorTemplate -addControl "leftHandThumbTip";
		editorTemplate -addControl "leftHandIndexTip";
		editorTemplate -addControl "leftHandMiddleTip";
		editorTemplate -addControl "leftHandRingTip"; 
		editorTemplate -addControl "leftHandPinkyTip";
		editorTemplate -addControl "leftHandExtraFingerTip";
		editorTemplate -addControl "rightHandThumbTip";
		editorTemplate -addControl "rightHandIndexTip";
		editorTemplate -addControl "rightHandMiddleTip";
		editorTemplate -addControl "rightHandRingTip"; 
		editorTemplate -addControl "rightHandPinkyTip";
		editorTemplate -addControl "rightHandExtraFingerTip";		
		editorTemplate -endLayout;
						
		editorTemplate -beginLayout (uiRes("m_AEhikHandleTemplate.kToeTipsSize"))  -collapse 1; 
		editorTemplate -addControl "leftFootThumbTip";
		editorTemplate -addControl "leftFootIndexTip";
		editorTemplate -addControl "leftFootMiddleTip";
		editorTemplate -addControl "leftFootRingTip"; 
		editorTemplate -addControl "leftFootPinkyTip";
		editorTemplate -addControl "leftFootExtraFingerTip";
		editorTemplate -addControl "rightFootThumbTip";
		editorTemplate -addControl "rightFootIndexTip";
		editorTemplate -addControl "rightFootMiddleTip";
		editorTemplate -addControl "rightFootRingTip"; 
		editorTemplate -addControl "rightFootPinkyTip";
		editorTemplate -addControl "rightFootExtraFingerTip";		
		editorTemplate -endLayout;
		
		editorTemplate -endLayout; // end HIK handle attrs

		// include/call base class/node attributes
		AEtransformMain $nodeName;
		AEtransformNoScroll($nodeName);

		// suppressed attributes
		editorTemplate -suppress "ikBlend";
		editorTemplate -suppress "joints";
		editorTemplate -suppress "time";
		editorTemplate -suppress "effectors";
		editorTemplate -suppress "defaultMatrix";
		editorTemplate -suppress "mentalRayControls";
		editorTemplate -suppress "ikSolver";
