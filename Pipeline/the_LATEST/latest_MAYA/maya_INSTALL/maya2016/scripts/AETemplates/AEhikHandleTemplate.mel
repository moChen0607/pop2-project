		editorTemplate -suppress "contactsPosition";
		editorTemplate -suppress "propertyChanged";
		editorTemplate -suppress "handsFloorContactSetup";
		editorTemplate -suppress "feetFloorContactSetup";
		editorTemplate -suppress "toesFloorContactSetup";
		editorTemplate -suppress "fingersFloorContactSetup";		
		editorTemplate -suppress "fingerTipsSizes";	
		editorTemplate -suppress "toeTipsSizes";	
