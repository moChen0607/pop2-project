$around =1.000;#0.00,20.00
$upNdown =6.000;#1.00,10.00
$Xgrain =40.000;#0.00,100.00
$sharpness =1.896;#1.00,4.00
$Grain = 0.700;#0.0,1.0

$braids = abs(sin(PI*$around*($u -($upNdown* $v))));
