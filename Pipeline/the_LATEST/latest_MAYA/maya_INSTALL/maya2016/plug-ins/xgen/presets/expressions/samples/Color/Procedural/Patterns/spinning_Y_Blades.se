$frame = 1.000;#1,100
$rotate = $frame*10;
$blades = 2.000;#1,8
$v = (atan2d($P[1], $P[0]) + $rotate)*$blades/360 % 1;
$u = length($P)->invert();
$a = 0.112;
$b = 0.000;
$c = 0.356;#.01,1
$seed = 55.000;#1,100
$freq = 20.000;#5,100
$blade = gaussstep($v, 0, $a) * gaussstep($v, $a + $b +$c, $a + $b);
$noise = noise($u*$freq+$seed*100);
$noiseAmt = 0.488;
mix($blade, $blade * $noise, $noiseAmt) #* ($u > .5)
