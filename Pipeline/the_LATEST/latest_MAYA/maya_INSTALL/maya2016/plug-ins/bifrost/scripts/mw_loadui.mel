global proc mw_loadui()
{
    eval( "BifrostRegisterRuntimeCmd" ); 

	eval ("source \"menu_MakeBifrost.mel\"");
	eval ("MakeBifrost");

	MakeBifrostHUD;
}

global proc mw_unloadui()
{
	RemoveBifrost;
	RemoveBifrostHUD;
}

global proc BifrostRegisterRuntimeCmd()
{
	if (!`runTimeCommand -exists CreateBifrostLiquid`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostLiquidAV3"
                CreateBifrostLiquid;
    }
	if (!`runTimeCommand -exists CreateBifrostAero`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostAeroAV3"
                CreateBifrostAero;
    }
	if (!`runTimeCommand -exists AddBifrostEmitter`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostEmitter \"Emitters\" "
                AddBifrostEmitter;
    }
	if (!`runTimeCommand -exists AddBifrostCollider`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostCollider \"Colliders\" "
                AddBifrostCollider;
    }
	if (!`runTimeCommand -exists AddBifrostAccelerator`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostAccelerator \"Accelerators\" "
                AddBifrostAccelerator;
    }
	if (!`runTimeCommand -exists AddBifrostKillplane`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostKillplane \"killplanes\" "
                AddBifrostKillplane;
    }
	if (!`runTimeCommand -exists AddBifrostFoam`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostFoamAV3 "
                AddBifrostFoam;
    }
	if (!`runTimeCommand -exists AddBifrostCamera`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostCamera \"cameraMatrixList\" "
                AddBifrostCamera;
    }
	if (!`runTimeCommand -exists AddBifrostAdaptiveMesh`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostAdaptiveMesh \"AdaptiveMeshes\" "
                AddBifrostAdaptiveMesh;
    }
	if (!`runTimeCommand -exists AddBifrostGuide`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "addBifrostGuide \"Guides\" "
                AddBifrostGuide;
    }

	if (!`runTimeCommand -exists RemoveBifrostEmitter`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostEmitter \"Emitters\" "
                RemoveBifrostEmitter;
    }
	if (!`runTimeCommand -exists RemoveBifrostCollider`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostCollider \"Colliders\" "
                RemoveBifrostCollider;
    }
	if (!`runTimeCommand -exists RemoveBifrostAccelerator`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostAccelerator \"Accelerators\" "
                RemoveBifrostAccelerator;
    }
	if (!`runTimeCommand -exists RemoveBifrostFoam`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostFoam"
                RemoveBifrostFoam;
    }
	if (!`runTimeCommand -exists RemoveBifrostKillplane`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostKillplane \"killplanes\" "
                RemoveBifrostKillplane;
    }
	if (!`runTimeCommand -exists RemoveBifrostAdaptiveMesh`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostAdaptiveMesh \"AdaptiveMeshes\" "
                RemoveBifrostAdaptiveMesh;
    }
	if (!`runTimeCommand -exists RemoveBifrostGuide`) {
        runTimeCommand -default true
                -category "Menu items.FX.Bifrost"
                -command   "removeBifrostGuide \"Guides\" "
                RemoveBifrostGuide;
    }


}
