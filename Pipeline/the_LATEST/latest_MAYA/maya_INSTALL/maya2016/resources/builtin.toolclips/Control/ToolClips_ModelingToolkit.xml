<?xml version="1.0" encoding="utf-8"?>
<toolclips ui="ui/ToolClipUI.qml" simple="ui/SimpleTooltip.qml" locale="en_US">
  <toolclip key="nexPowerBtn" title="Activate the Modeling Toolkit" help="TC-nexPowerBtn">
    <text>Enables the Modeling Toolkit's selection and transformation tools.</text>
  </toolclip>
  <toolclip key="nexbevelBtn" title="Bevel Tool" help="TC-nexbevelBtn">
    <text>Expands the selected polygon components into new faces.</text>
    <expanded>
      <resourcelist>
        <image location="MTK_Bevel.png">Bevel edges</image>
        <link location="TC-Bevel-polygon-edges-and-faces">Bevel polygon edges and faces</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexbridgeBtn" title="Bridge Tool" help="TC-nexbridgeBtn">
    <text>Creates bridging faces between selected edges or faces.</text>
    <expanded>
      <resourcelist>
        <image location="MTK_Bridge.png">Bridge between edges</image>
        <link location="TC-Bridge-between-border-edges">Bridge between border edges</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexweldBtn" title="Target Weld Tool" help="TC-nexweldBtn">
    <text>Merges two edges or vertices into a single component.</text>
    <expanded>
      <resourcelist>
        <image location="MTK_TargetWeld.png">Weld vertices</image>
        <link location="TC-Merge-polygon-components">Merge polygon components</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexextrudeBtn" title="Extrude Tool" help="TC-nexextrudeBtn">
    <text>Pulls new polygons from existing faces, edges, or vertices.</text>
    <expanded>
      <resourcelist>
        <image location="MTK_Extrude.png">Extrude faces</image>
        <link location="TC-Extrude-a-polygon-face-edge-or-vertex">Extrude a polygon face, edge, or vertex</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexquaDrawBtn" title="Quad Draw Tool" help="TC-nexquaDrawBtn">
    <text>Tools to retopologize your mesh.</text>
    <expanded>
      <default-resource index="1" />
      <resourcelist>
        <video location="QuadDraw_MakeLive.webm">Set a reference surface</video>
        <video location="QuadDraw_Drop.webm">Create quads</video>
        <video location="QuadDraw_ExtendEdges.webm">Extend edges</video>
        <video location="QuadDraw_Weld.webm">Auto-weld components</video>
        <image location="QuadDraw_Relax.png">Relax vertices</image>
        <link location="TC-Quad-Draw-Tool">Quad Draw Tool</link>
        <link location="TC-Retopologize-a-mesh-with-Quad-Draw">Retopologize a mesh with Quad Draw</link>
        <link location="TC-Quad-Draw-Tool-hotkeys">Quad Draw Tool hotkeys</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexconnectBtn" title="Connect Tool" help="TC-Connect-Tool">
    <text>Connects the selected polygon components by inserting an edge between them.</text>
    <expanded>
      <resourcelist>
        <image location="MTK_Connect.png">Connect components</image>
        <link location="TC-Add-edges-between-polygon-components">Add edges between polygon components</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexcutBtn" title="Multi-Cut Tool" help="TC-Multi-Cut-Tool">
    <text>Tools to cut, slice, and insert edges on polygons.</text>
    <expanded>
      <default-resource index="1" />
      <resourcelist>
        <image location="MultiCut_Slice.png">Slice a mesh</image>
        <image location="MultiCut_Cut.png">Cut faces</image>
        <link location="TC-Slice-a-mesh-with-the-Multi-Cut-Tool">Slice a mesh with the Multi-Cut Tool</link>
        <link location="TC-Cut-faces-with-the-Multi-Cut-Tool">Cut faces with the Multi-Cut Tool</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexSymLabel" title="Symmetry" help="TC-Symmetry">
    <text>Activates symmetry.</text>
    <expanded>
      <resourcelist>
        <video location="Symmetry_ActivateTopo.webm">Activate topological symmetry</video>
        <link location="TC-Symmetrical-editing">Symmetrical editing</link>
        <link location="TC-Activate-or-deactivate-symmetry">Activate or deactivate symmetry</link>
        <link location="TC-Edit-multiple-shells-with-topological-symmetry">Edit multiple shells with topological symmetry</link>
        <link location="TC-Edit-a-mesh-with-topological-symmetry">Edit a mesh with topological symmetry</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexSymPopupButton" title="Symmetry" help="TC-nexSymPopupButton">
    <text>Activates symmetry.</text>
    <expanded>
      <resourcelist>
        <video location="Symmetry_ActivateTopo.webm">Activate topological symmetry</video>
        <link location="TC-Symmetrical-editing">Symmetrical editing</link>
        <link location="TC-Activate-or-deactivate-symmetry">Activate or deactivate symmetry</link>
        <link location="TC-Edit-multiple-shells-with-topological-symmetry">Edit multiple shells with topological symmetry</link>
        <link location="TC-Edit-a-mesh-with-topological-symmetry">Edit a mesh with topological symmetry</link>
		</resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nexSymTextField" title="Symmetry" help="TC-nexSymTextField">
    <text>Activates symmetry.</text>
    <expanded>
      <resourcelist>
        <video location="Symmetry_ActivateTopo.webm">Activate topological symmetry</video>
        <link location="TC-Symmetrical-editing">Symmetrical editing</link>
        <link location="TC-Activate-or-deactivate-symmetry">Activate or deactivate symmetry</link>
        <link location="TC-Edit-multiple-shells-with-topological-symmetry">Edit multiple shells with topological symmetry</link>
        <link location="TC-Edit-a-mesh-with-topological-symmetry">Edit a mesh with topological symmetry</link>
      </resourcelist>
    </expanded>
  </toolclip>
  <toolclip key="nextranConstLabel" title="Transform Constraints" help="TC-nextranConstLabel">
    <text>Restricts transformations to the edges or surface of the selected mesh. </text>
  </toolclip>
  <toolclip key="nextranConstPopupButton" title="Transform Constraints" help="TC-nextranConstPopupButton">
    <text>Restricts transformations to the edges or surface of the selected mesh. </text>
  </toolclip>
  <toolclip key="nextranConstTextField" title="Transform Constraints" help="TC-nextranConstTextField">
    <text>Restricts transformations to the edges or surface of the selected mesh. </text>
  </toolclip>
  <toolclip key="nexselConstLabel" title="Selection Constraints" help="TC-nexselConstLabel">
    <text>Conditions to filter your selections.</text>
  </toolclip>
  <toolclip key="nexselConstField" title="Selection Constraints" help="TC-nexselConstField">
    <text>Conditions to filter your selections.</text>
  </toolclip>
  <toolclip key="nexselConstPopupButton" title="Selection Constraints" help="TC-nexselConstPopupButton">
    <text>Conditions to filter your selections.</text>
  </toolclip>
  <toolclip key="nexselConstTextField" title="Selection Constraints" help="TC-nexselConstTextField">
    <text>Conditions to filter your selections.</text>
  </toolclip>
</toolclips>