<config_root>
    <mirror>
       <mirror_list>
            <item key="Left" value="Right" />
            <item key="L " value="R " />
            <item key="L_" value="R_" />
            <item key="_L" value="_R" />
        </mirror_list>
    </mirror>
    <name_match>
        <delimiter_list>
            <item value="_" />
            <item value=":" />
            <item value="." />
        </delimiter_list>
        <match_type value="Suffix" />
    </name_match>
</config_root>